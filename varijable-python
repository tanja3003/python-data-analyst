#using function input(prompt)
greeting = "Welcome to the Band Name Generator."
question1 = "What's the name of the city you grew up in?"
question2 = "What's your pet's name?"
result = "Your band name could be "
print(greeting)
print(result + input(question1) + " " + input(question2))